<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Member">
	<select id="isMember" parameterType="Member" resultType="String">
		select password from sje_member where user_id=#{user_id} and password=#{password}
	</select>

	<insert id="signUp">
		insert into sje_member values(#{user_id},#{user_name},#{password},now())
	</insert>
	
		<select id="getInfo" parameterType="String" resultType="Member">
		select * from sje_member where user_id=#{user_id}
	</select>
</mapper>